{
  "expo": {
    "name": "tadado",
    "slug": "tadado",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tadado",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "fonts": ["./assets/fonts/Inter-VariableFont_opsz,wght.ttf"]
    },
    "android": {
      "fonts": [
        {
          "fontFamily": "Inter",
          "fontDefinitions": [
            {
              "path": "./assets/fonts/Inter-VariableFont_opsz,wght.ttf",
              "weight": 700
            }
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
